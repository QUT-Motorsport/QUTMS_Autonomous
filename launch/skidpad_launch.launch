<?xml version="1.0"?>
<launch>
    <!-- Some environment params-->
    <arg name="debug" default="false"/>
    <arg name="gui" default="true"/>


    <!-- Gazebo specifications -->
    <include file="$(find gazebo_ros)/launch/empty_world.launch">    
        <arg name="world_name" value="$(find qutms_autonomous)/worlds/qev2_drive.world"/>
        <arg name="debug" value="$(arg debug)"/>
        <arg name="gui" value="$(arg gui)"/>
        <arg name="paused" value="false"/>
        <arg name="use_sim_time" value="true"/>
        <arg name="headless" value="false"/>
    </include>

    <!-- Grab a model file for a track-->
    <param name="track_name" value="/home/nick-pc-ubuntu/.gazebo/models/Skidpad_test_track/model.sdf"/>
    <node name="spawn_sdf" pkg="gazebo_ros" type="spawn_model" args="-file /home/nick-pc-ubuntu/.gazebo/models/Skidpad_test_track/model.sdf -sdf -model skidpad"/>

</launch>
